<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute">

    <font
        android:font="@font/kanit_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/kanit_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <font
        android:font="@font/kanit_italic"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/kanit_italic"
        app:fontStyle="italic"
        app:fontWeight="400" />

    <font
        android:font="@font/kanit_light"
        android:fontStyle="normal"
        android:fontWeight="200"
        app:font="@font/kanit_light"
        app:fontStyle="normal"
        app:fontWeight="200" />

    <font
        android:font="@font/kanit_light_italic"
        android:fontStyle="italic"
        android:fontWeight="200"
        app:font="@font/kanit_light_italic"
        app:fontStyle="italic"
        app:fontWeight="200" />

</font-family>